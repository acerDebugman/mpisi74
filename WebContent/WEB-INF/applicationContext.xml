<!--===== Trigger =====-->
	<bean id="quartzTrigger1" class="org.springframework.scheduling.quartz.CronTriggerFactoryBean">
		<property name="jobDetail">
		  <ref bean="quartzDetail1"/>
		</property>
		<property name="cronExpression">
			<value>0 0 1 * * ?</value>
			<!--  <value>0 0/15 * * * ?</value> -->
		</property>
	</bean>
	<bean id="quartzTrigger2" class="org.springframework.scheduling.quartz.CronTriggerFactoryBean">
