<!-- 系统定时任务执行处理 -->
<!--===== job =====-->
	<bean id="executeJobs" class="schedule.executeJobs"/>
<!--===== job detail =====-->
	<bean id="quartzDetail1" class="org.springframework.scheduling.quartz.MethodInvokingJobDetailFactoryBean">
		<property name="targetObject">
		  <ref bean="executeJobs"/>
		</property>
		<property name="targetMethod">
		  <value>executeJob1</value>
