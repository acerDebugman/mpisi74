		</property>
	</bean>
<!--===== Schedule =====-->
	<bean autowire="no" class="org.springframework.scheduling.quartz.SchedulerFactoryBean">
		<property name="triggers">
			<list>
			    <ref local="quartzTrigger1"/>
			    <ref local="quartzTrigger2"/>
			    <!-- <ref local="quartzTrigger3"/> -->
			    <ref local="quartzTrigger4"/>
			    <ref local="quartzTrigger5"/>
			    <ref local="quartzTrigger6"/>
			    <ref local="quartzTrigger7"/>
			    <!-- <ref local="quartzTrigger8"/> -->
			    <ref local="quartzTrigger9"/>
			    <ref local="quartzTrigger10"/>
			</list>
		</property>
	</bean>
