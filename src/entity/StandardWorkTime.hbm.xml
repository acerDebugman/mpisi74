<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="entity.StandardWorkTime" table="StandardWorkTime">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="native" />
        </id>
        <!-- <property name="employeeNum" type="java.lang.String">
            <column name="employeeNum" length="30" not-null="false" />
        </property> -->
        <property name="dayDate" type="java.util.Date">
            <column name="dayDate" not-null="false" />
        </property>
        <!-- <property name="circleDayIdx" type="java.lang.Integer">
            <column name="circleDayIdx" not-null="false" />
        </property> -->
        <property name="stdClockInTime" type="java.util.Date">
            <column name="stdClockInTime" not-null="false" />
        </property>
        <property name="stdClockOutTime" type="java.util.Date">
            <column name="stdClockOutTime" not-null="false" />
        </property>
        <property name="manuallyChangeFlag" type="java.lang.Boolean">
        	<column name="manuallyChangeFlag" not-null="false" />
        </property>
        <property name="comments" type="java.lang.String">
        	<column name="comments" not-null="false" />
        </property>
        <property name="lateMinutes" type="java.lang.Integer">
        	<column name="lateMinutes" not-null="false" />
        </property>
        <property name="earlyMinutes" type="java.lang.Integer">
        	<column name="earlyMinutes" not-null="false" />
        </property>
        
        <many-to-one name="employee" class="entity.MP1001" column="employeeCode">
        </many-to-one>
        <many-to-one name="circleDay" class="entity.EachCircleDay" column="circleDayId"></many-to-one>
        <many-to-one name="detailChangeLog" class="entity.MP0011" not-null="false" column="manuallyChangeLogId" unique="true"></many-to-one>
    </class>
</hibernate-mapping>