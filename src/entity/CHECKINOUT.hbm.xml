<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="entity.CHECKINOUT" table="CHECKINOUT">
        <composite-id>
	        <key-property name="USERID" type="java.lang.Integer">
	            <column name="USERID" not-null="false" />
	        </key-property>
	        <key-property name="CHECKTIME" type="java.lang.String">
	            <column name="CHECKTIME" length="30" not-null="false" />
	        </key-property>
        </composite-id>
        <property name="CHECKTYPE" type="java.lang.String">
            <column name="CHECKTYPE" length="1" not-null="false" />
        </property>
        <property name="VERIFYCODE" type="java.lang.Integer">
            <column name="VERIFYCODE" not-null="false" />
        </property>
        <property name="SENSORID" type="java.lang.String">
            <column name="SENSORID" length="5" not-null="false" />
        </property>
        <property name="MEMOINFO" type="java.lang.String">
            <column name="MEMOINFO" length="30" not-null="false" />
        </property>
        <property name="WORKCODE" type="java.lang.Integer">
            <column name="WORKCODE" not-null="false" />
        </property>
        <property name="SN" type="java.lang.String">
            <column name="SN" length="20" not-null="false" />
        </property>
        <property name="USEREXTFMT" type="java.lang.Integer">
            <column name="USEREXTFMT" not-null="false" />
        </property>
    </class>
</hibernate-mapping>

