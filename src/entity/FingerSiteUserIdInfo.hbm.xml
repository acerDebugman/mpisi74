<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="entity.FingerSiteUserIdInfo" table="FingerSiteUserIdInfo">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="native" />
        </id>
        <property name="userId" type="java.lang.Integer">
            <column name="userId" not-null="false" />
        </property>
        <property name="branchSite" type="java.lang.String">
            <column name="branchSite" not-null="false" />
        </property>
        
        <many-to-one name="employee" class="entity.MP1001" fetch="select">
        	<column name="employeeCode" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>