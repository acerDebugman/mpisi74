<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="entity.MP2004" table="MP2004">
        <id name="MP2004_SEQ" type="java.lang.String">
            <column name="MP2004_SEQ" />
            <generator class="assigned" />
        </id>
        <property name="MP2004_EMPLOYEE_NUM" type="java.lang.String">
            <column name="MP2004_EMPLOYEE_NUM" length="10" not-null="false" />
        </property>
        <property name="MP2004_MAJOR_NAME" type="java.lang.String">
            <column name="MP2004_MAJOR_NAME" length="200" not-null="false" />
        </property>
        <property name="MP2004_TIME" type="java.lang.String">
            <column name="MP2004_TIME" length="5" not-null="false" />
        </property>
        <property name="MP2004_STATUS" type="java.lang.String">
            <column name="MP2004_STATUS" length="5" not-null="false" />
        </property>
    </class>
</hibernate-mapping>
