<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="entity.HOLIDAY" table="HOLIDAY">
        <id name="HOLIDAY_SEQ" type="java.lang.Integer">
            <column name="HOLIDAY_SEQ" />
            <generator class="native" />
        </id>
        <property name="HOLIDAY_NAME" type="java.lang.String">
            <column name="HOLIDAY_NAME" length="100" not-null="false" />
        </property>
        <property name="HOLIDAY_DATE" type="java.lang.String">
            <column name="HOLIDAY_DATE" length="10" not-null="false" />
        </property>
        <property name="HOLIDAY_YEAR" type="java.lang.String">
            <column name="HOLIDAY_YEAR" length="4" not-null="false" />
        </property>
    </class>
</hibernate-mapping>