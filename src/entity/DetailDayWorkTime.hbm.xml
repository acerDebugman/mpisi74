<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="entity.DetailDayWorkTime" table="DetailDayWorkTime">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="native" />
        </id>
        <property name="name" type="java.lang.String">
            <column name="name" length="255" not-null="false" />
        </property>
        <property name="fromTime" type="java.util.Date">
            <column name="fromTime" not-null="false" />
        </property>
        <property name="toTime" type="java.util.Date">
            <column name="toTime" not-null="false" />
        </property>
        <!-- <property name="eachCircleDayId" type="java.lang.Integer">
            <column name="eachCircleDayId" not-null="false" />
        </property> -->
        
      	<many-to-one name="circleDay" class="entity.EachCircleDay" fetch="select">
      		<column name="eachCircleDayId" not-null="false" />
      	</many-to-one>
    </class>
</hibernate-mapping>
